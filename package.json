{
  "name": "@hectare/platform",
  "version": "1.0.0",
  "author": "developers@hectare.farm",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write .",
    "test": "jest --watchAll --no-cache --detectOpenHandles",
    "clean": "rm -rf build",
    "compile": "tsc -p tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "postinstall": "bit link",
    "postuninstall": "bit link",
    "debug": "ts-node-dev --inspect=0.0.0.0:9001 --respawn --transpile-only --watch components,app,integrations,modules app/index.ts"
  },
  "dependencies": {
    "@bugsnag/js": "7.16.1",
    "@bugsnag/plugin-aws-lambda": "7.16.1",
    "@hapi/hapi": "20.2.1",
    "@middy/core": "2.5.7",
    "aws-sdk": "2.1089.0",
    "mongoose": "6.2.4",
    "mongoose-paginate-v2": "1.6.3",
    "openapi-backend": "5.2.1",
    "ravendb": "5.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.93",
    "@types/jest": "26.0.22",
    "@types/js-yaml": "4.0.5",
    "@types/mongoose-paginate-v2": "1.4.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.23.2",
    "hapi": "18.1.0",
    "jest": "26.6.3",
    "prettier": "2.4.1",
    "prettier-eslint": "12.0.0",
    "pretty-quick": "3.1.3",
    "source-map-support": "0.5.21",
    "ts-node-dev": "1.1.8",
    "typescript": "4.2.4"
  }
}
